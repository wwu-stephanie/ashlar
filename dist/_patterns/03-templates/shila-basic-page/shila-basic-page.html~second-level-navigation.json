{
    "page": {
        "include()": {
            "pattern": "organisms-shila-page-html",
            "with": {
                "page": {
                    "header": {
                        "include()": {
                            "pattern": "organisms-shila-page-header-html",
                            "with": {
                                "content": {
                                    "join()": [
                                        "organisms-shila-branding-html",
                                        "molecules-shila-navigation-html-main",
                                        {
                                            "include()": {
                                                "pattern": "molecules-shila-navigation-html",
                                                "with": {
                                                    "configuration": {
                                                        "label_display": false
                                                    }
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "content": {
                        "include()": {
                            "pattern": "organisms-shila-page-content-html",
                            "with": {
                                "content": {
                                    "join()": [
                                        {
                                            "include()": {
                                                "pattern": "organisms-shila-block-html-page-title",
                                                "with": {
                                                    "content": {
                                                        "include()": {
                                                            "pattern": "molecules-shila-page-title-html",
                                                            "with": {
                                                                "title": "Page with second level navigation"
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        },
                                        {
                                            "include()": {
                                                "pattern": "organisms-shila-block-html-main",
                                                "with": {
                                                    "content": "organisms-shila-node-html-page"
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "footer": "organisms-shila-page-footer-html"
                }
            }
        }
    },
    "off_canvas": "organisms-shila-off-canvas-html"
}
