<div{{ attributes.addClass(classes) }}>
  {% if multiple %}
    <div class="field__items">
  {% endif %}
  {% for item in items %}
    <div{{ title_attributes.addClass(title_classes) }}>{{ item.title }}</div>
    <div{{ item.attributes.addClass('field__item') }}>{{ item.content }}</div>
  {% endfor %}
  {% if multiple %}
    </div>
  {% endif %}
</div>
