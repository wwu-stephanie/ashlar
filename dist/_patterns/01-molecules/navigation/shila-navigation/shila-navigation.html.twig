{#
/**
 * @see templates/block/block--system-menu-block.html.twig
 */
#}
<nav role="navigation" aria-labelledby="{{ heading_id }}"{{ attributes }}>
  {{ title_prefix }}
  {# Label. If not displayed, we still provide it for screen readers. #}
  {% if not configuration.label_display %}
    <div class="visually-hidden">
  {% endif %}
  <h2{{ title_attributes }}>{{ configuration.label }}</h2>
  {% if not configuration.label_display %}
    </div>
  {% endif %}
  {{ title_suffix }}

  {# Menu. #}
  {% block content %}
    {{ content }}
  {% endblock %}
</nav>
