<div class="local-tasks">
  {% if primary %}
    <h2 class="visually-hidden">{{ primary_tabs_title }}</h2>
    <ul class="local-tasks__primary">{{ primary }}</ul>
  {% endif %}
  {% if secondary %}
    <h2 class="visually-hidden">{{ secondary_tabs_title }}</h2>
    <ul class="local-tasks__secondary">{{ secondary }}</ul>
  {% endif %}
</div>
