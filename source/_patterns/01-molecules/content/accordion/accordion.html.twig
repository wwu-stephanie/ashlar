<div{{ attributes }}>
  {% for item in items %}
  <div class="item">
    <h3{{ title_attributes }}>
      <button class="expand">
        <span class="material-icons" aria-hidden="true">add</span>
        {{ item.title }}
      </button>
    </h3>

    <div class="content">{{ item.content }}</div>
  </div>
  {% endfor %}
</div>
