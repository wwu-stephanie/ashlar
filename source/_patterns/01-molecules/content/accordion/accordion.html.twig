<div{{ attributes.addClass(classes) }}>
  {% for item in items %}
  <div class="item">
    <button class="expand">
      <span class="material-icons">add</span>
      <h3{{ title_attributes.addClass(title_classes) }}>{{ item.title }}</h3>
    </button>
    <div{{ item.attributes.addClass('field__item', 'content') }}>{{ item.content }}</div>
  </div>
  {% endfor %}
</div>
