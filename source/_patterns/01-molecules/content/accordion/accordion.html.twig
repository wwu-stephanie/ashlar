<div{{ attributes.addClass(classes) }}>
  {% for item in items %}
  <div class="item">
  <h3{{ title_attributes.addClass(title_classes) }}>
    <button class="expand">
      <span class="material-icons" aria-hidden="true">add</span>
      {{ item.title }}
    </button>
    </h3>
    <div{{ item.attributes.addClass('field__item', 'content') }}>{{ item.content }}</div>
  </div>
  {% endfor %}
</div>
