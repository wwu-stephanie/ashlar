<ul role="tablist" aria-label="Content Switcher" {{ attributes.addClass(classes) }}>
  {% for item in items %}
  <li><button role="tab" {{ item.attributes.addClass(item.classes) }}>{{ item.title }}</button></li>
  {% endfor %}
</ul>

<div role="tabpanel" class="content-switcher-container">
  {% for item in items %}
  <div{{ item.attributes.addClass("content", item.classes) }}><p>{{ item.content }}</p></div>
  {% endfor %}
</div>
