{
    "attributes": {
        "Attribute()": {
            "id": "search-block-form",
            "class": ["search-block-form"]
        }
    },
    "children": {
        "join()": [
            {
                "include()": {
                    "pattern": "molecules-shila-form-element-html-textfield",
                    "with": {
                        "label": {
                            "include()": {
                                "pattern": "atoms-shila-label-html",
                                "with": {
                                    "attributes": {
                                        "Attribute()": {
                                            "class": []
                                        }
                                    },
                                    "title_display": "invisible",
                                    "title": "Search"
                                }
                            }
                        }
                    }
                }
            },
            {
                "include()": {
                    "pattern": "atoms-shila-input-html-submit",
                    "with": {
                        "attributes": {
                            "Attribute()": {
                                "id": ["edit-submit"],
                                "type": ["submit"],
                                "value": ["Search"],
                                "class": ["button"]
                            }
                        }
                    }
                }
            }
        ]
    }
}
