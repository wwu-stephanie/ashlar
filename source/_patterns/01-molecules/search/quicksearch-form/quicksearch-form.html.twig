<div{{ attributes }}>
  {% include "@atoms/global/label/label.twig" %}

  {% block content %}
  <div class="body">
    {{ content.body }}
  </div>
  {% endblock %}

  {% block form %}
    <form autocomplete="off" id="quick-search-form">
      <label for="quick-search-input">{{ content.field_search_label }}</label>
      <input id="quick-search-input" type="search" />
    </form>
  {% endblock %}

  {% block list_nav %}
    <p class="visually-hidden">Filter by all results or matching letter.</p>
    <div id="quick-search-list-nav"></div>
  {% endblock %}
</div>
