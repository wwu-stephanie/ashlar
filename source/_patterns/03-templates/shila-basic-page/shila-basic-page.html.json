{
    "page": {
        "include()": {
            "pattern": "organisms-page-html",
            "with": {
                "page": {
                    "header": "organisms-site-header-html",
                    "content": {
                        "include()": {
                            "pattern": "organisms-content-region-html",
                            "with": {
                                "content": {
                                    "join()": [
                                        {
                                            "include()": {
                                                "pattern": "organisms-block-html-page-title",
                                                "with": {
                                                    "content": {
                                                        "include()": {
                                                            "pattern": "molecules-page-title-html",
                                                            "with": {
                                                                "title": "Basic page"
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        },
                                        {
                                            "include()": {
                                                "pattern": "organisms-block-html-main",
                                                "with": {
                                                    "content": "organisms-shila-node-html-page"
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "footer": "organisms-wwu-footer-html"
                }
            }
        }
    },
    "off_canvas": "organisms-shila-off-canvas-html"
}
