{% for item in items %}
<div class="double-margin--bottom">
  <button {{ attributes }} data-lightbox-url="{{ item.data.url }}" data-lightbox-title="{{ item.data.title }}" data-cta-url="{{ item.cta.link }}"data-cta-text="{{ item.cta.text }}">
    <span aria-hidden="true" class="play-icon">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 26.87 31.03">
        <polygon class="cls-1" points="26.87 15.52 0 0 0 31.03 26.87 15.52"/>
      </svg>
    </span>
    <span class="visually-hidden">
      {{ item.button.name }}
    </span>
  </button>
</div>
{% endfor %}
