<button{{ attributes }} data-lightbox-url="{{ item.data.url }}" data-lightbox-title="{{ item.data.title }}" data-cta-url="{{ item.cta.link }}"data-cta-text="{{ item.cta.text }}">
    <span aria-hidden="true" class="play-icon">
      <svg viewBox="0 0 44.56 44.56" xmlns="http://www.w3.org/2000/svg"><path d="m22.28 0a22.28 22.28 0 1 0 22.28 22.28 22.28 22.28 0 0 0 -22.28-22.28zm2 27-8.19 4.7v-18.85l8.16 4.72 8.16 4.71z"></path></svg></span> 
    <span class="visually-hidden">{{ item.button.name }}</span> 
  </button>
  </div>
{% endfor %}